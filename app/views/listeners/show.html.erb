<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <%= @listener.query %>
      <% @listener.social_watchers.each do |watcher| %>
        <% if watcher.reddit? %>
          Reddit
        <% elsif watcher.hacker_news_story? %>
          Hacker News
        <% end %>
      <% end %>
    </div>
  </div>
</div>
